{
  "name": "glamor",
  "version": "2.17.7",
  "description": "inline css for component systems",
  "main": "lib/index.js",
  "jsnext:main": "es6/index.js",
  "typings": "index.d.ts",
  "files": [
    "es6",
    "lib",
    "umd",
    "aphrodite.js",
    "jsxstyle.js",
    "react.js",
    "ous.js",
    "reset.js",
    "utils.js",
    "server.js",
    "index.d.ts",
    "aphrodite.d.ts",
    "jsxstyle.d.ts",
    "ous.d.ts",
    "react.d.ts",
    "utils.d.ts"
  ],
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --config webpack/dev.js",
    "build": "npm run clean && npm run build:lib && npm run build:es6 && mkdir umd && npm run build:umd && npm run size",
    "build:lib": "babel src -d lib --presets es2015,stage-0,react",
    "build:es6": "babel src -d es6 --presets es2015-rollup,stage-0,react",
    "build:umd": "webpack --config webpack/umd.js && npm run compress:umd",
    "build:umd-react": "echo 'react build not implemented'",
    "compress:umd": "NODE_ENV=production webpack --config webpack/umd.js",
    "clean": "rm -rf lib && rm -rf es6 && rm -rf umd",
    "test": "NODE_ENV=test node ./node_modules/.bin/karma start tests/karma.conf.js --single-run",
    "test:server": "NODE_ENV=test babel-node tests/server.js",
    "test:watch": "NODE_ENV=test node ./node_modules/.bin/karma start tests/karma.conf.js",
    "prepublish": "npm run build",
    "size": "cat umd/index.min.js | gzip | wc -c"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "classnames": "^2.2.5",
    "eslint": "^3.7.1",
    "eslint-config-rackt": "^1.1.1",
    "eslint-plugin-react": "^6.4.1",
    "expect": "^1.20.2",
    "expect-jsx": "^2.6.0",
    "jade": "^1.11.0",
    "karma": "^1.3.0",
    "karma-browserstack-launcher": "^1.1.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-safari-launcher": "^1.0.0",
    "karma-webpack": "^1.8.0",
    "minimist": "^1.2.0",
    "mocha": "^3.1.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.9"
  },
  "author": "Sunil Pai <threepointone@gmail.com>",
  "homepage": "",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "css",
    "inline"
  ],
  "eslintConfig": {
    "extends": [
      "rackt"
    ],
    "plugins": [
      "react"
    ],
    "rules": {
      "react/jsx-uses-vars": "error",
      "react/jsx-uses-react": "error",
      "jsx-quotes": 0
    }
  }
}
